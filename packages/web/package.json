    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-relay": "^11.0.2",
    "before-build-webpack": "^0.2.11",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^7.0.2",
    "execa": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
