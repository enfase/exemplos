{
  "name": "@example/web",
  "description": "Web template with GraphQL Relay",
  "private": true,
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start-dev": "yarn relay && env-cmd -f .env.development webpack serve --config webpack.dev.js",
    "relay": "relay-compiler",
    "update": "yarn relay",
    "prebuild": "yarn --frozen-lockfile && rm -rf build && yarn relay",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 env-cmd -f .env.production webpack --config webpack.prod.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@date-io/moment": "^2.10.8",
    "@example/ui": "*",
    "@example/utils": "*",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "core-js": "^3.12.1",
    "formik": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-relay": "^11.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "relay-config": "^11.0.2",
    "relay-hooks": "^4.2.0",
    "relay-runtime": "^11.0.2",
    "styled-components": "^5.3.0",
    "yup": "^0.32.9"
