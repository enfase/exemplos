{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "license": "ISC",
  "scripts": {
    "delete:node_modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "delete:build": "find . -name \"build\" -exec rm -rf '{}' +",
    "clear": "yarn delete:node_modules && yarn delete:build",
    "test": "jest"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@template/eslint-config": "*",
    "@types/jest": "^26.0.5",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
